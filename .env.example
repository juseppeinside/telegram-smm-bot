# Токен Telegram бота (получить у @BotFather)
BOT_TOKEN=your_bot_token_here

# Интервал отправки медиа файлов (в миллисекундах)
# По умолчанию 3600000 = 1 час
MEDIA_INTERVAL=3600000

# Путь для сохранения медиа файлов
MEDIA_FOLDER=./media 

# ID канала, в который будут отправляться медиа файлы
# Например: -1001234567890
TARGET_CHANNEL_ID= 